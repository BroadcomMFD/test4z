 MODE  AMODE(31),RMODE(ANY)
 ENTRY CUSTDB
 NAME  CUSTDB(R)
/*
//* ------------------------------------------------------------------
//*        BIND                    -----------------------------------
//* ------------------------------------------------------------------
//BIND    EXEC PGM=IKJEFT01,DYNAMNBR=20,COND=(4,LT)
