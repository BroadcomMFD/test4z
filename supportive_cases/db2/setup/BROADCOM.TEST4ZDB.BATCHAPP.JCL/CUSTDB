//SYSTSIN   DD *
  DSN SYSTEM($$subsystem)
  BIND PACKAGE(DBPCKG) MEMBER(CUSTDB)
  BIND PLAN(DBPLAN) PKLIST(DBPCKG.*)
  END
/*
//* ------------------------------------------------------------------
//*              RUN                       ---------------------------
//* ------------------------------------------------------------------
